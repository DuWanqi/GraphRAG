# LLM API Keys Configuration
# 复制此文件为 .env 并填入您的API密钥

# Deepseek API
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_BASE=https://api.deepseek.com/v1

# Qwen (阿里云通义千问)
QWEN_API_KEY=your_qwen_api_key_here
QWEN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1

# 腾讯混元
HUNYUAN_API_KEY=your_hunyuan_api_key_here
HUNYUAN_SECRET_ID=your_hunyuan_secret_id_here
HUNYUAN_SECRET_KEY=your_hunyuan_secret_key_here

# Google AI Studio (Gemini)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI (可选，用于GraphRAG默认配置)
OPENAI_API_KEY=your_openai_api_key_here

# 默认使用的LLM模型
DEFAULT_LLM_PROVIDER=deepseek
DEFAULT_LLM_MODEL=deepseek-chat

# GraphRAG配置
GRAPHRAG_INPUT_DIR=./data/input
GRAPHRAG_OUTPUT_DIR=./data/output
